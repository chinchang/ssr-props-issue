{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://monorepo-blog-chi.vercel.app"
    },

    {
      "source": "/api/socialImage/target",
      "destination": "/api/targetSocialImage"
    }
  ],
  "headers": [
    {
      "source": "/api/socialImage/target",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=62, s-maxage=62"
        }
      ]
    }
  ]
}
